package com.company;

public class a7 {
    public static void main(String[] args){
        float[] x = {7.89f,4.67f,2.67f};
        float r = 4.67f;
        boolean isarray= false;
        for(float q:x){
            if(q==r) {
                isarray = true;
                break;
            }
        }
        if(isarray){
            System.out.println("the value is present in array");
        }
        else
            System.out.println("the value is not preset in the array");
    }
}
